<p id="notice"><%= notice %></p>

<h1>Deep Data</h1>

<table>
  <thead>
    <tr>
      <th>Turtle</th>
      <th>Temperature</th>
      <th>Deep</th>
      <th>Light</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deep_data.each do |deep_datum| %>
      <tr>
        <td><%= deep_datum.turtle %></td>
        <td><%= deep_datum.temperature %></td>
        <td><%= deep_datum.deep %></td>
        <td><%= deep_datum.light %></td>
        <td><%= deep_datum.latitude %></td>
        <td><%= deep_datum.longitude %></td>
        <td><%= link_to 'Show', deep_datum %></td>
        <td><%= link_to 'Edit', edit_deep_datum_path(deep_datum) %></td>
        <td><%= link_to 'Destroy', deep_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deep Datum', new_deep_datum_path %>
