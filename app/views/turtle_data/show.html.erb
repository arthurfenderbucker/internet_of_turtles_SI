<div class="form-mine">
  <h1 class="display-4 text-center"><b>Show User</b></h1>
  <%= form_with model: @turtle_datum do |form| %>
    <fieldset disabled>

      <div class="form-group">
        <%= form.label :turtle_id %>
        <%= form.text_field :turtle_id, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :temperature %>
        <%= form.text_field :temperature, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :light %>
        <%= form.text_field :light, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :depth %>
        <%= form.text_field :depth, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :latitude %>
        <%= form.text_field :latitude, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :longitude %>
        <%= form.text_field :longitude, class: "form-control" %>
      </div>
    </fieldset>
  <% end %>
  <br/>
  <%= link_to 'Edit', edit_turtle_datum_path(@turtle_datum), class: "btn btn-primary form-control"  %>
  <br/>
  <br/>
  <%= link_to 'Show turtles datum', turtle_data_path, class: "btn btn-light form-control"  %>

</div>



